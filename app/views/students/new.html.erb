<form action="/students" method="POST">
  <label>Name</label>
  <input name="full_name" placeholder=<%= @placeholder_name %> type="text"></input>
  <label>StudentID</label>
  <input
    name="student_id"
    placeholder=<%= @placeholder_student_id %>
    type="text"
  >

  </input>
  <label>Major</label>
  <input name="major" placeholder=<%= @placeholder_major %> type="text"></input>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <button type="submit">Enter</button>
</form>
